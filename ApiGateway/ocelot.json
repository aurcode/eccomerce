{
  "GlobalConfiguration": {
    "BaseUrl": "https://localhost:44372"
  },
  "Routes": [
    {
      "UpstreamPathTemplate": "/token",
      "UpstreamHttpMethod": [ "Get", "Post" ],
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7158
        }
      ],
      "DownstreamPathTemplate": "/token",
      "SwaggerKey": "token",
      "Key": "token"
    },
    {
      "UpstreamPathTemplate": "/api/e/{everything}",
      "UpstreamHttpMethod": [],
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7165
        }
      ],
      "DownstreamPathTemplate": "/api/{everything}",
      "SwaggerKey": "inventoryAdmin",
      "Key": "inventoryAdmin",
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "Bearer"
      },
      "RouteClaimsRequirement": {
        "userType": "admin"
      }
    },
    {
      "UpstreamPathTemplate": "/api/{everything}",
      "UpstreamHttpMethod": [ "GET" ],
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7165
        }
      ],
      "DownstreamPathTemplate": "/api/{everything}",
      "SwaggerKey": "inventoryUser",
      "Key": "inventoryUser",
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "Bearer"
      },
      "RouteClaimsRequirement": {
        "userType": "user"
      }      
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "token",
      "Config": [
        {
          "Name": "token API",
          "Version": "v1",
          "Url": "https://localhost:7158/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "inventoryUser",
      "Config": [
        {
          "Name": "Inventory User API",
          "Version": "v1",
          "Url": "https://localhost:7165/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "inventoryAdmin",
      "Config": [
        {
          "Name": "Inventory Admin API",
          "Version": "v1",
          "Url": "https://localhost:7165/swagger/v1/swagger.json"
        }
      ]
    }    
  ]
}